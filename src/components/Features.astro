---
import {
  Card,
  CardContent,
  CardDescription,
  CardFooter,
  CardHeader,
  CardTitle,
} from '../components/ui/card'
---

<section id="apps" class=", flex gap-10">
  <Card className="w-[350px]">
    <CardHeader>
      <CardTitle
        ><img
          class="w-80"
          src="/Edge-sense.png"
          alt=""
        /></CardTitle
      >
      <CardDescription><h3>Deploy your new project in one-click.</h3></CardDescription>
    </CardHeader>
    <CardContent>
      <div class="grid w-full items-center gap-4">
        <div class="flex flex-col space-y-1.5"></div>
        <div class="flex flex-col space-y-1.5"></div>
      </div>
    </CardContent>
    <CardFooter className="flex justify-between" />
  </Card>
</section>
